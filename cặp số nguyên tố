#include<bits/stdc++.h>
using namespace std;
int const maxn=1e5;
bool nt[maxn+1];
void sieve(){
	memset(nt,true,sizeof(nt));
	for(int i=2;i<=sqrt(maxn+1);i++){
		if(nt[i]){
			for(int j=i*i;j<=maxn+1;j+=i){
				nt[j]=false;
			}
		}
	}
}
int main(){
	sieve();
	int t;cin>>t;
	while(t--){
		int n;cin>>n;
		for(int i=2;i<=n/2;i++){
			if(nt[i]&&nt[n-i]){
				cout<<i<<" "<<n-i;
				break;
			}
		}
		cout<<endl;
	}
}
